{
  "name": "PyTorch 3 Rocm 7.1",
  "dockerComposeFile": "pytorch-rocm-compose.yaml",
  "service": "pytorch-rocm",
  "onCreateCommand": {
    "update": "apt update && apt -y upgrade"
  },
  "postAttachCommand": {
    "setup-env": ".devcontainer/setup-env.sh"
  },
  "customizations": {
    "jetbrains": {
      "backend": "PyCharm",
      "plugins": [
        "PythonCore", "Pythonid"
      ]
    }
  }
  // Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
  // "remoteUser": "root"
}
